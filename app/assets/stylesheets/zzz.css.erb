  display: table-cell;
  vertical-align: top;
  padding: 0 12px 0 12px;
}
.content-cols .row2 .sidebar {
  border-right: 1px solid #eeeeee;
  width: 160px;
}
.content-cols .sidebar input,
.content-cols .sidebar select {
  height: 30px;
  padding: 5px 10px;
  font-size: 12px;
  border-radius: 3px;
}
.content-cols .sidebar .btn {
  padding: 5px 10px;
  font-size: 12px;
  line-height: 1.5;
  border-radius: 3px;
}

.content-cols .row2 .main {
  width: 600px;
}
.content-cols .row2 .meta {
  display: none;
}

