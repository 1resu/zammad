body {
  font-family: "FiraSansOT", "Helvetica Neue", Helvetica, Arial, sans-serif;
  line-height: 1.7;
}

#app > nav {
  position: relative;
  z-index: 1;
}

.glyphicon {
  font-size: 13px;
}

.spinner {
  float: left;
  background: url("/assets/images/spinner.gif") no-repeat;
  background-position: center center;
  padding: 44px 10px 0 10px;
  width: 10px;
  height: 14px;
  display: none;
}

table {
  table-layout: fixed;
}
table th, table td {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.table {
  display: table;
}
.table .table-row {
  display: table-row;
}
.table .table-cell{
  vertical-align: top;
  display: table-cell;
  border-bottom: 1px solid #eeeeee;
}

.popover {
  min-width: 240px;
  z-index: 1110;
}
.hero-two {
  width: 100%;
}
.hero-two .hero-left {
  width: 50%;
  float: left;
}
.hero-two .hero-right {
  width: 50%;
  float: right;
}

.content-one {
  top: 78px;
  margin-left: 20px;
  margin-right: 20px;
