<div class="page-header">
  <div class="row">
    <div class="span9">
      <h1 class="pull-left"><%- @T( 'New Ticket' ) %> <small></small></h1>
    </div>
    <div class="span3">
      <div class="btn-group pull-right" data-toggle="buttons-radio">
        <button type="button" class="btn article-type-phone active"><i class="icon-headphones"></i></button>
        <button type="button" class="btn article-type-email"><i class="icon-envelope"></i></button>
      </div>
    </div>
  </div>
</div>
<form class="form-horizontal">
  <div class="row">
    <div class="span9" id="form_create"></div>
    <div class="span3">
      <div id="customer_info"></div>
      <div id="ticket_template"></div>
      <div id="text_module"></div>
    </div>
  </div>
  <div class="form-actions">
    <button type="reset" class="btn cancel"><%- @T( 'Cancel' ) %></button>&nbsp;<button type="submit" class="btn btn-primary submit"><%- @T( 'Create' ) %></button>
  </div>
</form>
