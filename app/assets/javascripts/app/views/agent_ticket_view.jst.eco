<div class="page-header">
  <div class="row">
    <div class="span9">
      <h1><%- @T( @overview.meta.name ) %> <% if @edit: %><small><a href="#" data-type="settings" class="icon-edit"></a></small><% end %></h1>
    </div>
    <div class="span3">
      <div class="pagination keepright">
        <ul>
          <% for item in @view_modes: %>
            <li class="<%= item.class %>">
              <a href="#" data-type="view-mode" data-mode="<%= item.type %>"><%= item.name %></a>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
