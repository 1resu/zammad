<div class="sidebar <%= @className %>">

  <% if @groups: %>
  <% for group in @groups: %>
  <h2><%- @T(group.name) %></h2>

  <ul class="nav nav-pills nav-stacked">
  <% if group.items: %>
  <% for item in group.items: %>
    <li <% if item.active: %>class="active"<% end %>><a href="<%= item.target %>"><%- @T(item.name) %></a></li>
  <% end %>
  <% end %>
  </ul>
  <% end %>

</div>
<div class="main flex"></div>