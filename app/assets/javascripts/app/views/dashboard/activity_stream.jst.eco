<div class="span3">
    <h2 class="can-move"><%- @T( @head ) %></h2>
    <dl>
    <% for item in @items: %>
      <dt><span class="user-data" data-id="<%= item.updated_by_id %>">"<%= item.updated_by.displayName() %>"</span></dt>
      <dd><%- @T( item.history_type ) %> <a href="<%- item.link %>"><%= item.type %><% if item.title: %> (<%= item.title %>)<% end %></a>.</dd>
    <% end %>
    </dl>
</div>
