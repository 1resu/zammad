<div class="row">
  <div class="span3">
    <h2><%= @head %></h2>
    <ul>
    <% for item in @items: %>
      <li><span class="user-data" data-id="<%= item.created_by_id %>">"<%= item.created_by.firstname %> <%= item.created_by.lastname %>"</span> <%= item.history_type.name %> <span data-id="<%= item.o_id %>"><a data-type="edit" href="#"><%= item.history_object.name %> (<%= item.ticket.title %>)</a></span>.</li>
    <% end %>
    </ul>
  </div>
</div>